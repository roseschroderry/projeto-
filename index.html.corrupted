        // Buscar relat√≥rio do Google Sheets Cache (mantendo compatibilidade com IndexedDB legado)
        async function getReportFromDB(reportType) {
            // Primeiro tentar pegar do cache do Google Sheets
            if (window.reportDataCache[reportType] && window.reportDataCache[reportType].length > 0) {
                return {
                    reportType: reportType,
                    data: window.reportDataCache[reportType],
                    uploadedAt: new Date().toISOString(),
                    fileName: `${reportType}_google_sheets.csv`
                };
            }
            
            // Fallback para IndexedDB (caso ainda haja dados antigos)
            return new Promise((resolve, reject) => {
                if (!db) {
                    resolve(null);
                    return;
                }
                const transaction = db.transaction(['files'], 'readonly');
                const store = transaction.objectStore('files');
                const request = store.getAll();
                
                request.onsuccess = () => {
                    const files = request.result || [];
                    const report = files.find(f => f.reportType === reportType);
                    resolve(report);
                };
                
                request.onerror = () => reject(request.error);
            });
        }

