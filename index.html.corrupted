                width: auto;
                padding: 12px 16px;
                font-size: 14px;
            }

            /* File upload */
            #fileInput {
                font-size: 13px;
                padding: 10px;
            }

            /* Scroll suave em mobile */
            .content-section {
                overflow-x: hidden;
            }

            /* Ajustar espa√ßamento vertical */
            .nav-section {
                margin-bottom: 20px;
            }
        }

        /* Extra small devices */
        @media (max-width: 480px) {
            .auth-container {
                padding: 20px 16px;
                margin: 8px;
            }

            .card {
                padding: 14px;
            }

