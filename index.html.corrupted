                    ];
                    
                    // Adicionar bot√£o de carregamento manual
                    const loadBtn = document.createElement('button');
                    loadBtn.id = 'btnCarregarRelatorios';
                    loadBtn.innerHTML = 'üîÑ CARREGAR RELAT√ìRIOS';
                    loadBtn.style.cssText = 'position:fixed;top:70px;right:20px;z-index:9999;padding:12px 24px;background:linear-gradient(135deg,#10B981,#059669);color:white;border:none;border-radius:8px;font-size:14px;font-weight:600;cursor:pointer;box-shadow:0 4px 12px rgba(16,185,129,0.4);transition:all 0.3s;';
                    loadBtn.onmouseover = () => { loadBtn.style.transform = 'translateY(-2px)'; loadBtn.style.boxShadow = '0 6px 16px rgba(16,185,129,0.5)'; };
                    loadBtn.onmouseout = () => { loadBtn.style.transform = 'translateY(0)'; loadBtn.style.boxShadow = '0 4px 12px rgba(16,185,129,0.4)'; };
                    loadBtn.onclick = async function() {
                        this.disabled = true;
                        this.innerHTML = '‚è≥ Carregando...';
                        this.style.background = '#6B7280';
                        
                        try {
                            // Limpa o cache
                            window.reportDataCache = {};
                            console.log('üîÑ Carregando relat√≥rios do Google Sheets...');
                            
                            // Carrega todos os relat√≥rios em paralelo
                            const promises = reportsConfig.map(async (config) => {
                                try {
                                    const response = await fetch(config.url, { cache: "no-store" });
                                    
                                    if (!response.ok) throw new Error(`Erro HTTP ${response.status}`);
                                    
                                    const text = await response.text();
                                    window.reportDataCache[config.id] = parseRobustCSV(text);
                                    
                                    console.log(`‚úÖ ${config.label} (${window.reportDataCache[config.id].length} linhas)`);
                                } catch (err) {
                                    console.error(`‚ùå Erro em ${config.label}:`, err);
                                    window.reportDataCache[config.id] = [];
                                }
                            });

                            await Promise.all(promises);
                            
                            // Tentar salvar no localStorage (pode falhar se dados forem muito grandes)
                            try {
                                localStorage.setItem('googleSheetsCache', JSON.stringify(window.reportDataCache));
                                localStorage.setItem('googleSheetsLastUpdate', new Date().toISOString());
                                console.log('üíæ Dados salvos no localStorage');
                            } catch (storageErr) {
                                console.warn('‚ö†Ô∏è N√£o foi poss√≠vel salvar no localStorage (dados muito grandes):', storageErr.message);
                            }
                            
                            const totalRelatorios = Object.keys(window.reportDataCache).length;
                            const relatoriosComDados = Object.values(window.reportDataCache).filter(d => d && d.length > 0).length;
                            const totalLinhas = Object.values(window.reportDataCache).reduce((sum, data) => sum + (data?.length || 0), 0);
                            
                            console.log(`‚úÖ ${relatoriosComDados}/${totalRelatorios} relat√≥rios carregados com sucesso, ${totalLinhas} linhas`);
                            
                            this.innerHTML = '‚úÖ CONCLU√çDO!';
                            this.style.background = 'linear-gradient(135deg,#10B981,#059669)';
                            setTimeout(() => this.remove(), 2000);
                            
                            const relatoriosFalharam = totalRelatorios - relatoriosComDados;
                            let mensagem = `‚úÖ ${relatoriosComDados} relat√≥rios carregados!\n\n${totalLinhas.toLocaleString('pt-BR')} linhas no total.`;
                            
                            if (relatoriosFalharam > 0) {
                                mensagem += `\n\n‚ö†Ô∏è ${relatoriosFalharam} relat√≥rios falharam (verifique o console para detalhes).`;
                            }
                            
                            alert(mensagem + '\n\nAgora voc√™ pode abrir os relat√≥rios dispon√≠veis.');
                            
                        } catch(err) {
                            console.error('‚ùå Erro ao carregar:', err);
                            this.innerHTML = '‚ùå ERRO - Clique novamente';
                            this.style.background = '#EF4444';
                            this.disabled = false;
                            alert('‚ùå Erro ao carregar dados:\n\n' + err.message + '\n\nTente novamente.');
                        }
                    };
                    document.body.appendChild(loadBtn);
                    
                    // Tentar carregar automaticamente tamb√©m
                    if (typeof window.carregarDadosDoSheets === 'function') {
                        window.carregarDadosDoSheets();
                    }
                }, 1000);
            }, 100);
            
            console.log('‚úÖ Login OK');
        };
        
        window.toggleMobileMenu = function() {
            try {
                const sidebar = document.getElementById('sidebar');
                const overlay = document.getElementById('mobileOverlay');
                const menuBtn = document.getElementById('mobileMenuBtn');
                const menuIcon = document.getElementById('menuIcon');
                
                if (!sidebar) return;
                
                const isOpen = sidebar.classList.contains('mobile-open');
                
                if (isOpen) {
                    sidebar.classList.remove('mobile-open');
                    if (overlay) overlay.classList.remove('active');
                    if (menuBtn) menuBtn.classList.remove('open');
                    if (menuIcon) menuIcon.textContent = '‚ò∞';
                    document.body.style.overflow = '';
                } else {
                    sidebar.classList.add('mobile-open');
                    if (overlay) overlay.classList.add('active');
                    if (menuBtn) menuBtn.classList.add('open');
                    if (menuIcon) menuIcon.textContent = '√ó';
                    document.body.style.overflow = 'hidden';
                }
            } catch (error) {
                console.error('‚ùå Erro menu:', error);
            }
        };
        
        window.closeMobileMenu = function() {
            try {
                const sidebar = document.getElementById('sidebar');
                const overlay = document.getElementById('mobileOverlay');
                const menuBtn = document.getElementById('mobileMenuBtn');
                const menuIcon = document.getElementById('menuIcon');
                
                if (sidebar) sidebar.classList.remove('mobile-open');
                if (overlay) overlay.classList.remove('active');
                if (menuBtn) menuBtn.classList.remove('open');
                if (menuIcon) menuIcon.textContent = '‚ò∞';
                document.body.style.overflow = '';
            } catch (error) {
                console.error('‚ùå Erro:', error);
            }
        };
        
