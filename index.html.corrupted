        // Ler planilha Excel ou CSV
        async function readSpreadsheet(file) {
            return new Promise((resolve, reject) => {
                const reader = new FileReader();
                const fileExt = file.name.split('.').pop().toLowerCase();
                
                reader.onload = (e) => {
                    try {
                        if (fileExt === 'csv') {
                            // Processar CSV
                            const text = e.target.result;
                            const lines = text.split('\n');
                            const headers = lines[0].split(',').map(h => h.trim());
                            
                            const data = [];
                            for (let i = 1; i < lines.length; i++) {
                                if (lines[i].trim()) {
                                    const values = lines[i].split(',');
                                    const row = {};
                                    headers.forEach((header, index) => {
                                        row[header] = values[index]?.trim() || '';
                                    });
                                    data.push(row);
                                }
                            }
                            resolve(data);
                            
                        } else {
                            // Para Excel, vamos simular dados por enquanto
                            // Em produ√ß√£o, use biblioteca como SheetJS (xlsx)
                            showNotification('‚ö†Ô∏è Formato Excel detectado. Use CSV para melhor compatibilidade.', 'info');
                            resolve([{
                                info: 'Arquivo Excel detectado',
                                nota: 'Converta para CSV para visualiza√ß√£o completa'
                            }]);
                        }
                    } catch (error) {
                        reject(error);
                    }
                };
                
                if (fileExt === 'csv') {
                    reader.readAsText(file);
                } else {
                    reader.readAsArrayBuffer(file);
                }
            });
        }

        // Salvar relat√≥rio no IndexedDB
        async function saveReportToDB(reportData) {
            return new Promise((resolve, reject) => {
                const transaction = db.transaction(['files'], 'readwrite');
                const store = transaction.objectStore('files');
                
                // Remover relat√≥rio anterior do mesmo tipo
                const getAllRequest = store.getAll();
                getAllRequest.onsuccess = () => {
                    const files = getAllRequest.result || [];
                    const oldReport = files.find(f => f.reportType === reportData.reportType);
                    
                    if (oldReport && oldReport.id) {
                        store.delete(oldReport.id);
                    }
                    
                    // Adicionar novo relat√≥rio
                    const addRequest = store.add(reportData);
                    addRequest.onsuccess = () => resolve();
                    addRequest.onerror = () => reject(addRequest.error);
                };
            });
        }

