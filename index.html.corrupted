        // AI File Analysis
        function analyzeFileWithAI(file) {
            const fileType = getFileType(file.name);
            const size = (file.size / 1024).toFixed(2);
            
            let analysis = {
                type: fileType,
                category: categorizeFile(fileType),
                size: size + ' KB',
                insights: [],
                tags: []
            };
            
            // Categoriza√ß√£o inteligente
            if (['pdf', 'doc', 'docx', 'txt', 'md'].includes(fileType)) {
                analysis.category = 'üìÑ Documento';
                analysis.insights.push('Documento de texto identificado');
                analysis.insights.push('Recomendado para leitura e an√°lise de conte√∫do');
                analysis.tags = ['documento', 'texto', 'leitura'];
            } else if (['xls', 'xlsx', 'csv'].includes(fileType)) {
                analysis.category = 'üìä Planilha';
                analysis.insights.push('Planilha de dados detectada');
                analysis.insights.push('Cont√©m dados estruturados para an√°lise');
                analysis.tags = ['dados', 'planilha', 'an√°lise'];
            } else if (['png', 'jpg', 'jpeg', 'gif'].includes(fileType)) {
                analysis.category = 'üñºÔ∏è Imagem';
                analysis.insights.push('Arquivo de imagem identificado');
                analysis.insights.push('Formato: ' + fileType.toUpperCase());
                analysis.tags = ['imagem', 'visual', 'm√≠dia'];
            } else if (['json', 'xml'].includes(fileType)) {
                analysis.category = '‚öôÔ∏è Dados';
                analysis.insights.push('Arquivo de dados estruturados');
                analysis.insights.push('Formato de configura√ß√£o ou API');
                analysis.tags = ['dados', 'estruturado', 'config'];
            } else if (fileType === 'zip') {
                analysis.category = 'üì¶ Arquivo';
                analysis.insights.push('Arquivo compactado detectado');
                analysis.insights.push('Cont√©m m√∫ltiplos arquivos');
                analysis.tags = ['compactado', 'm√∫ltiplos', 'arquivo'];
            }
            
            // An√°lise de tamanho
            if (file.size < 1024 * 100) {
                analysis.insights.push('Arquivo pequeno - processamento r√°pido');
            } else if (file.size < 1024 * 1024 * 5) {
                analysis.insights.push('Arquivo m√©dio - tempo de processamento moderado');
            } else {
                analysis.insights.push('Arquivo grande - pode requerer mais tempo');
            }
            
            return analysis;
        }
        
        function getFileType(filename) {
            const ext = filename.split('.').pop().toLowerCase();
            return ext;
        }
        
        function categorizeFile(type) {
            const categories = {
                'pdf': 'üìÑ Documento',
                'doc': 'üìÑ Documento',
                'docx': 'üìÑ Documento',
                'txt': 'üìÑ Documento',
                'md': 'üìÑ Documento',
                'xls': 'üìä Planilha',
                'xlsx': 'üìä Planilha',
                'csv': 'üìä Planilha',
                'png': 'üñºÔ∏è Imagem',
                'jpg': 'üñºÔ∏è Imagem',
                'jpeg': 'üñºÔ∏è Imagem',
                'gif': 'üñºÔ∏è Imagem',
                'json': '‚öôÔ∏è Dados',
                'xml': '‚öôÔ∏è Dados',
                'zip': 'üì¶ Arquivo'
            };
            return categories[type] || 'üìÑ Arquivo';
        }
        
