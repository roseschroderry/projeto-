        // =================================================================================
        window.carregarDadosDoSheets = async function() {
            console.log("üì• Iniciando carregamento via API...");
            
            // Garantir que o cache existe
            if (!window.reportDataCache) {
                window.reportDataCache = {};
                console.log('üì¶ Inicializando reportDataCache');
            }
            
            // Mostrar indicador de carregamento no chat
            const chatMessages = document.getElementById('chatMessages');
            if (chatMessages) {
                const loadingMsg = document.createElement('div');
                loadingMsg.className = 'message bot-message';
                loadingMsg.id = 'loading-reports-message';
                loadingMsg.innerHTML = `
                    <div class="message-content">
                        <div class="message-avatar">ü§ñ</div>
                        <div class="message-text">
                            <div style="display: flex; align-items: center; gap: 10px;">
                                <div class="loading" style="width: 20px; height: 20px;"></div>
                                <span>Carregando relat√≥rios via API...</span>
                            </div>
                        </div>
                    </div>
                `;
                chatMessages.appendChild(loadingMsg);
                chatMessages.scrollTop = chatMessages.scrollHeight;
            }
            
            // Usar API local se dispon√≠vel, sen√£o API de produ√ß√£o
            const isLocalhost = window.location.hostname === 'localhost' || window.location.hostname === '127.0.0.1';
            const API_BASE = isLocalhost ? 'http://localhost:3000' : 'https://chat-ai-backend-1.onrender.com';
            
            console.log('üîç Ambiente detectado:', {
                hostname: window.location.hostname,
                isLocalhost,
                API_BASE,
                usarAPI: !!API_BASE
            });
            
            if (API_BASE) {
                // Carregar via API
                try {
                    console.log('üîÑ Carregando via API:', API_BASE);
                    const response = await fetch(`${API_BASE}/reports`, { cache: "no-store" });
                    
                    if (!response.ok) throw new Error(`Erro HTTP ${response.status}`);
                    
                    const responseData = await response.json();
                    
                    // A API retorna {total, category, reports: Array}
                    const reports = responseData.reports || responseData;
                    console.log(`üìä ${reports.length} relat√≥rios encontrados`);
                    
                    // Carregar dados de cada relat√≥rio
                    for (const report of reports) {
                        try {
                            const dataResponse = await fetch(`${API_BASE}/reports/${report.id}/data`, { cache: "no-store" });
                            if (dataResponse.ok) {
                                const data = await dataResponse.json();
                                window.reportDataCache[report.id] = data;
                                console.log(`‚úÖ API: ${report.label} (${data.length} linhas)`);
                                console.log(`‚úÖ API: ${report.label} (${data.length} linhas)`);
                            }
                        } catch (err) {
                            console.error(`‚ùå Erro ao carregar ${report.id}:`, err);
                            window.reportDataCache[report.id] = [];
                        }
                    }
                } catch (err) {
                    console.error('‚ùå Erro na API, tentando Google Sheets:', err);
                    // Fallback para Google Sheets se API falhar
                    await carregarViaGoogleSheets();
                }
            } else {
                // Carregar via Google Sheets
                await carregarViaGoogleSheets();
            }
            
            async function carregarViaGoogleSheets() {
                const promises = REPORTS_CONFIG.map(async (config) => {
                    try {
                        const response = await fetch(config.url, { cache: "no-store" });
                        
                        if (!response.ok) throw new Error(`Erro HTTP ${response.status}`);
                        
                        const text = await response.text();
                        window.reportDataCache[config.id] = window.parseRobustCSV(text);
                        
                        console.log(`‚úÖ Sheets: ${config.label} (${window.reportDataCache[config.id].length} linhas)`);
                    } catch (err) {
                        console.error(`‚ùå Erro em ${config.label}:`, err);
                        window.reportDataCache[config.id] = [];
                    }
                });

                await Promise.all(promises);
            }
            
            // SALVAR no localStorage para persist√™ncia
            try {
                const reportCount = Object.keys(window.reportDataCache).length;
                const totalRows = Object.values(window.reportDataCache).reduce((sum, data) => sum + (data?.length || 0), 0);
                
                console.log(`üìä Total carregado: ${reportCount} relat√≥rios, ${totalRows} linhas`);
                console.log('üì¶ Cache:', Object.keys(window.reportDataCache));
                
                localStorage.setItem('googleSheetsCache', JSON.stringify(window.reportDataCache));
                localStorage.setItem('googleSheetsLastUpdate', new Date().toISOString());
                console.log('üíæ Dados salvos no localStorage');
            } catch (e) {
                console.warn('‚ö†Ô∏è Erro ao salvar cache:', e);
            }
            
            // Remover mensagem de carregamento
            const loadingMsg = document.getElementById('loading-reports-message');
            if (loadingMsg) {
                loadingMsg.remove();
            }
            
            // Adicionar mensagem de sucesso
            if (chatMessages) {
                const successMsg = document.createElement('div');
                successMsg.className = 'message bot-message';
                successMsg.innerHTML = `
                    <div class="message-content">
                        <div class="message-avatar">ü§ñ</div>
                        <div class="message-text">
                            ‚úÖ ${Object.keys(window.reportDataCache).length} relat√≥rios carregados! Digite <strong>ajuda</strong> para comandos.
                        </div>
                    </div>
                `;
                chatMessages.appendChild(successMsg);
                chatMessages.scrollTop = chatMessages.scrollHeight;
            }
            
            console.log("‚úÖ Todos os dados foram carregados e est√£o em 'window.reportDataCache'.");
            console.log("üìä Total de relat√≥rios:", Object.keys(window.reportDataCache).length);
            return window.reportDataCache;
        };

