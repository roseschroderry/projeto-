<body>
    <script>
        // ========== CONFIGURA√á√ÉO DA API BACKEND ==========
        // Detectar se est√° em localhost ou GitHub Pages
        const isLocal = window.location.hostname === 'localhost' || 
                       window.location.hostname === '127.0.0.1' ||
                       window.location.port === '8080';
        
        const API_URL = isLocal
            ? 'http://localhost:3000' 
            : 'https://chat-ai-backend-1.onrender.com';
        
        authToken = localStorage.getItem('api_token');
        let useBackend = localStorage.getItem('use_backend') === 'true';
        
        // Fun√ß√£o helper para requisi√ß√µes √† API
        window.apiRequest = async function(endpoint, options = {}) {
            const defaultOptions = {
                headers: {
                    'Content-Type': 'application/json',
                    ...(authToken && { 'Authorization': `Bearer ${authToken}` })
                }
            };
            
            const response = await fetch(`${API_URL}${endpoint}`, {
                ...defaultOptions,
                ...options,
                headers: {
                    ...defaultOptions.headers,
                    ...options.headers
                }
            });
            
            if (!response.ok) {
                const error = await response.text();
                throw new Error(error || 'Erro na requisi√ß√£o');
            }
            
            return response.json();
        };
        
        // Login com backend API
        window.doLoginAPI = async function() {
            const email = document.getElementById('loginEmail').value.trim();
            const password = document.getElementById('loginPassword').value;
            const btn = document.getElementById('loginBtn');
            
            if (!email || !password) {
                alert('Preencha email e senha!');
                return;
            }
            
            btn.disabled = true;
            btn.textContent = 'Autenticando...';
            
            try {
                const data = await apiRequest('/api/auth/login', {
                    method: 'POST',
                    body: JSON.stringify({ email, password })
                });
                
                authToken = data.token;
                localStorage.setItem('api_token', authToken);
                localStorage.setItem('user', JSON.stringify(data.user));
                localStorage.setItem('use_backend', 'true');
                useBackend = true;
                
                window.currentUser = data.user;
                showMainApp();
                
            } catch (error) {
                alert('‚ùå Erro no login: ' + error.message);
                btn.disabled = false;
                btn.textContent = 'Entrar';
            }
        };
        
        // Exportar relat√≥rio (PDF/Excel)
        window.exportarRelatorio = async function(tipo, codvd, vendedor, formato = 'excel') {
            if (!authToken) {
                alert('‚ö†Ô∏è Fa√ßa login com backend para usar esta funcionalidade');
                return;
            }
            
            try {
                const response = await fetch(`${API_URL}/api/relatorios/gerar`, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                        'Authorization': `Bearer ${authToken}`
                    },
                    body: JSON.stringify({
                        tipo,
                        codvd: codvd || '123',
                        vendedor: vendedor || '',
                        exportar: formato
                    })
                });
                
                if (!response.ok) throw new Error('Erro ao gerar relat√≥rio');
                
                const blob = await response.blob();
                const url = window.URL.createObjectURL(blob);
                const a = document.createElement('a');
                a.href = url;
                a.download = `relatorio_${tipo}_${Date.now()}.${formato === 'pdf' ? 'pdf' : 'xlsx'}`;
                a.click();
                window.URL.revokeObjectURL(url);
                
                alert('‚úÖ Relat√≥rio exportado com sucesso!');
                
            } catch (error) {
                alert('‚ùå Erro ao exportar: ' + error.message);
            }
        };
        
        // Carregar hist√≥rico do backend
        window.carregarHistorico = async function() {
            if (!authToken) return;
            
            try {
                const data = await apiRequest('/api/historico');
                const historico = data.historico || [];
                
                if (historico.length === 0) {
                    return '<p style="color: #64748B; text-align: center; padding: 20px;">Nenhum hist√≥rico dispon√≠vel</p>';
                }
                
                let html = '<div style="background: white; border-radius: 12px; padding: 20px; margin: 20px 0;">';
                html += '<h3 style="margin-bottom: 15px; color: #DC2626;">üìä Hist√≥rico de Consultas</h3>';
                html += '<table style="width: 100%; border-collapse: collapse;">';
                html += '<tr style="background: #F8FAFC; border-bottom: 2px solid #DC2626;">';
                html += '<th style="padding: 12px; text-align: left;">Data/Hora</th>';
                html += '<th style="padding: 12px; text-align: left;">Tipo</th>';
                html += '<th style="padding: 12px; text-align: left;">CODVD</th>';
                html += '<th style="padding: 12px; text-align: left;">Vendedor</th>';
                html += '<th style="padding: 12px; text-align: center;">Registros</th>';
                html += '</tr>';
                
                historico.slice(-20).reverse().forEach((item, index) => {
                    const bgColor = index % 2 === 0 ? '#FFFFFF' : '#F8FAFC';
                    const date = new Date(item.timestamp).toLocaleString('pt-BR');
                    html += `<tr style="background: ${bgColor}; border-bottom: 1px solid #E2E8F0;">`;
                    html += `<td style="padding: 10px;">${date}</td>`;
                    html += `<td style="padding: 10px;">${item.tipo}</td>`;
                    html += `<td style="padding: 10px;">${item.codvd}</td>`;
                    html += `<td style="padding: 10px;">${item.vendedor || '-'}</td>`;
                    html += `<td style="padding: 10px; text-align: center; font-weight: 600; color: #DC2626;">${item.registros}</td>`;
                    html += '</tr>';
                });
                
                html += '</table></div>';
                return html;
                
            } catch (error) {
                console.error('Erro ao carregar hist√≥rico:', error);
                return '';
            }
        };
        
        // ========== GOOGLE SHEETS API FUNCTIONS ==========
        
        // Fun√ß√£o auxiliar de espera
        window.esperar = async function(ms) {
            return new Promise(resolve => setTimeout(resolve, ms));
        };
        
        // Buscar relat√≥rio com retry autom√°tico
        window.buscarRelatorioComRetry = async function(reportId, maxTentativas = 6, intervaloMs = 3000) {
            for (let tentativa = 1; tentativa <= maxTentativas; tentativa++) {
                console.log(`üîÅ Tentativa ${tentativa} para ${reportId}`);
                
                // 1. Verifica status
                const statusRes = await fetch(`${API_URL}/api/status`);
                const status = await statusRes.json();
                
                if (status.loading) {
                    console.log('‚è≥ Backend carregando dados...');
                    await window.esperar(intervaloMs);
                    continue;
                }
                
                // 2. Busca relat√≥rio
                const res = await fetch(`${API_URL}/api/sheets/${reportId}`);
                
                if (res.ok) {
                    const data = await res.json();
                    console.log('‚úÖ Dados recebidos');
                    return data;
                }
                
                console.log('‚ö†Ô∏è Relat√≥rio ainda n√£o dispon√≠vel, aguardando...');
                await window.esperar(intervaloMs);
            }
            
            throw new Error(`‚ùå Falha ao obter ${reportId} ap√≥s v√°rias tentativas`);
        };
        
        // Buscar lista de planilhas dispon√≠veis
        window.buscarPlanilhas = async function() {
            try {
                const data = await window.apiRequest('/api/sheets');
                return data.sheets || [];
            } catch (error) {
                console.error('Erro ao buscar planilhas:', error);
                return [];
            }
        };
        
        // Buscar dados de uma planilha espec√≠fica (com retry)
        window.buscarDadosSheet = async function(reportId) {
            try {
                const data = await window.buscarRelatorioComRetry(reportId);
                return data;
            } catch (error) {
                console.error(`Erro ao buscar dados da planilha ${reportId}:`, error);
                throw error;
            }
        };
        
        // Recarregar todas as planilhas do Google Sheets
        window.recarregarSheets = async function() {
            try {
                const data = await window.apiRequest('/api/sheets/reload');
                console.log('‚úÖ Planilhas recarregadas:', data);
                return data;
            } catch (error) {
                console.error('Erro ao recarregar planilhas:', error);
                throw error;
            }
        };
        
        // Buscar dados por palavras-chave (busca em novos clientes ou queijo)
        window.buscarPorPalavraChave = async function(query) {
            const queryLower = query.toLowerCase();
            const planilhas = await window.buscarPlanilhas();
            
            // Encontra planilha que corresponde √†s keywords
            const planilhaMatch = planilhas.find(p => 
                p.keywords.some(kw => queryLower.includes(kw))
            );
            
            if (planilhaMatch && planilhaMatch.has_data) {
                return await window.buscarDadosSheet(planilhaMatch.id);
            }
            
            return null;
        };
        
        // ========== NOVOS ENDPOINTS DE CATEGORIAS ==========
        
        // Buscar todos os relat√≥rios dispon√≠veis (com filtro opcional de categoria)
        window.buscarRelatorios = async function(categoria = null) {
            try {
                const url = categoria 
                    ? `${API_URL}/api/reports?category=${categoria}`
                    : `${API_URL}/api/reports`;
                    
                const response = await fetch(url);
                if (!response.ok) {
                    throw new Error(`HTTP ${response.status}: ${response.statusText}`);
                }
                
                const data = await response.json();
                console.log(`‚úÖ ${data.total} relat√≥rios carregados${categoria ? ` (categoria: ${categoria})` : ''}`);
                return data;
            } catch (error) {
                console.error('Erro ao buscar relat√≥rios:', error);
                throw error;
            }
        };
        
        // Buscar categorias dispon√≠veis com relat√≥rios agrupados
        window.buscarCategorias = async function() {
            try {
                const response = await fetch(`${API_URL}/api/reports/categories`);
                if (!response.ok) {
                    throw new Error(`HTTP ${response.status}: ${response.statusText}`);
                }
                
                const data = await response.json();
                console.log(`‚úÖ ${data.total} categorias carregadas:`, data.categories.map(c => c.name).join(', '));
                return data;
            } catch (error) {
                console.error('Erro ao buscar categorias:', error);
                throw error;
            }
        };
        
        // Buscar relat√≥rio espec√≠fico usando endpoint direto Node.js
        window.buscarRelatorioDireto = async function(reportId) {
            try {
                const response = await fetch(`${API_URL}/api/sheets-direct/${reportId}`);
                if (!response.ok) {
                    throw new Error(`HTTP ${response.status}: ${response.statusText}`);
                }
                
                const data = await response.json();
                console.log(`‚úÖ Relat√≥rio ${reportId} carregado (${data.count} linhas, fonte: ${data.source})`);
                return data;
            } catch (error) {
                console.error(`Erro ao buscar relat√≥rio ${reportId}:`, error);
                throw error;
            }
        };
        
        // Filtrar relat√≥rios por m√∫ltiplas categorias
        window.buscarRelatoriosPorCategorias = async function(categorias) {
            try {
                const promises = categorias.map(cat => window.buscarRelatorios(cat));
                const results = await Promise.all(promises);
                
                // Combina todos os relat√≥rios
                const todosRelatorios = results.flatMap(r => r.reports);
                
                return {
                    total: todosRelatorios.length,
                    categories: categorias,
                    reports: todosRelatorios
                };
            } catch (error) {
                console.error('Erro ao buscar relat√≥rios por categorias:', error);
                throw error;
            }
        };
        
        // Exibir dados de planilha no chat
        window.exibirDadosSheet = function(data, containerId = 'chatMessages') {
            if (!data || !data.data || data.data.length === 0) {
                return '<p class="text-gray-500">Nenhum dado encontrado.</p>';
            }
            
            const headers = Object.keys(data.data[0]);
            const maxRows = 50; // Limitar exibi√ß√£o
            const displayData = data.data.slice(0, maxRows);
            
            let html = `
                <div class="bg-white rounded-lg shadow-lg p-4 my-3">
                    <h3 class="text-lg font-bold text-gray-800 mb-2">${data.label}</h3>
                    <p class="text-sm text-gray-600 mb-3">
                        Total: ${data.rows} registros | Exibindo: ${displayData.length}
                    </p>
                    <div class="overflow-x-auto">
                        <table class="min-w-full border-collapse border border-gray-300 text-sm">
                            <thead class="bg-gray-100">
                                <tr>
                                    ${headers.map(h => `<th class="border border-gray-300 px-3 py-2 text-left">${h}</th>`).join('')}
                                </tr>
                            </thead>
                            <tbody>
                                ${displayData.map(row => `
                                    <tr class="hover:bg-gray-50">
                                        ${headers.map(h => `<td class="border border-gray-300 px-3 py-2">${row[h] || '-'}</td>`).join('')}
                                    </tr>
                                `).join('')}
                            </tbody>
                        </table>
                    </div>
                    ${data.rows > maxRows ? `<p class="text-xs text-gray-500 mt-2">+ ${data.rows - maxRows} registros n√£o exibidos</p>` : ''}
                </div>
            `;
            
            return html;
        };
        
        // ========== FUN√á√ïES GLOBAIS DEFINIDAS ANTES DO HTML ==========
        // Estas fun√ß√µes precisam estar dispon√≠veis para os onclick
        
        // LOGIN LOCAL (padr√£o - sem backend)
        window.doLogin = async function() {
            console.log('üîê doLogin chamado');
            
            try {
                const email = document.getElementById('loginEmail').value.trim();
                const password = document.getElementById('loginPassword').value;
                const rememberMe = document.getElementById('rememberMe')?.checked || false;
                const useBackend = document.getElementById('useBackendApi')?.checked || false;
                const btn = document.getElementById('loginBtn');
                
                if (!email || !password) {
                    alert('Preencha email e senha!');
                    return;
                }
                
                btn.disabled = true;
                btn.innerHTML = 'Entrando...';
                
                // NOVO: Se marcou "Usar Backend API", fazer login no backend
                if (useBackend) {
                    try {
                        const response = await fetch(`${API_URL}/api/auth/login`, {
                            method: 'POST',
                            headers: { 'Content-Type': 'application/json' },
                            body: JSON.stringify({ email, password })
                        });
                        
                        if (response.ok) {
                            const data = await response.json();
                            window.authToken = data.token;
                            window.currentUser = data.user;
                            window.useBackend = true;
                            
                            localStorage.setItem('api_token', data.token);
                            localStorage.setItem('use_backend', 'true');
                            localStorage.setItem('user', JSON.stringify(data.user));
                            
                            alert(`‚úÖ Login com backend realizado!\nUsu√°rio: ${data.user.name}`);
                            showMainApp();
                            return;
                        } else {
                            throw new Error('Credenciais inv√°lidas no backend');
                        }
                    } catch (error) {
                        alert(`‚ùå Erro no login com backend: ${error.message}\n\nTentando login local...`);
                        // Continua para login local
                    }
                }
                
                // Login local (padr√£o)
                const users = JSON.parse(localStorage.getItem('registeredUsers') || '[]');
                const user = users.find(u => u.email === email && u.password === password);
                
                if (user) {
                    window.currentUser = user;
                    window.authToken = 'token_' + Date.now();
                    window.useBackend = false;
                    
                    const storage = rememberMe ? localStorage : sessionStorage;
                    storage.setItem('token', window.authToken);
                    storage.setItem('user', JSON.stringify(user));
                    storage.setItem('use_backend', 'false');
                    
                    showMainApp();
                    
                } else {
                    alert('‚ùå Credenciais inv√°lidas!');
                    btn.disabled = false;
                    btn.innerHTML = 'Entrar';
                }
                
            } catch (error) {
                console.error('Erro no login:', error);
                alert('Erro ao fazer login: ' + error.message);
                const btn = document.getElementById('loginBtn');
                if (btn) {
                    btn.disabled = false;
                    btn.innerHTML = 'Entrar';
                }
            }
        };
        
        // Fun√ß√£o auxiliar para mostrar app principal
        function showMainApp() {
            document.getElementById('authPage').style.display = 'none';
            document.getElementById('mainApp').style.display = 'flex';
            
            setTimeout(() => {
                const user = window.currentUser;
                if (!user) return;
                
                const userName = document.getElementById('userName');
                const userAvatar = document.getElementById('userAvatar');
                if (userName) userName.textContent = user.name;
                if (userAvatar) userAvatar.textContent = user.name.charAt(0).toUpperCase();
                
                if (user.role === 'admin') {
                    const adminSection = document.getElementById('adminSection');
                    if (adminSection) {
                        adminSection.style.display = 'block';
                        console.log('‚úÖ Menu admin ativado');
                    }
                }
                
                // Carregar dados do Google Sheets ou API
                setTimeout(() => {
                    // Fun√ß√£o parseCSV local
                    const parseRobustCSV = function(text) {
                        const rows = text.trim().replace(/^\uFEFF/, '').match(/(?:[^\r\n"]+|"[^"]*")+/g);
                        if (!rows || rows.length < 2) return [];
                        
                        const headers = rows[0].split(',').map(h => h.trim());
                        const regex = /(?:^|,)("(?:[^"]|"")*"|[^,]*)/g;
                        const res = [];
                        
                        for (let i = 1; i < rows.length; i++) {
                            if (!rows[i].trim()) continue;
                            const obj = {}; let m; let j = 0; regex.lastIndex = 0;
                            while ((m = regex.exec(rows[i])) !== null) {
                                if (j >= headers.length) break;
                                let v = m[1];
                                if (v.startsWith('"') && v.endsWith('"')) v = v.substring(1, v.length - 1).replace(/""/g, '"');
                                obj[headers[j]] = v.trim(); j++;
                                if (m.index === regex.lastIndex) regex.lastIndex++;
                            }
                            res.push(obj);
                        }
                        return res;
                    };
                    
                    // Definir configura√ß√£o dos relat√≥rios
                    const reportsConfig = [
                        { id: 'novos_clientes', label: 'Novos Clientes', url: 'https://docs.google.com/spreadsheets/d/e/2PACX-1vR9lG9sbtgRqV0PLkyjT8R9znpC9ECGurgfelIhn_q5BwgThg6SpdfE2R30obAAaawk0FIGLlBowjt_/pub?gid=0&single=true&output=csv' },
                        { id: 'queijo_reino', label: 'Queijo do Reino', url: 'https://docs.google.com/spreadsheets/d/e/2PACX-1vR9lG9sbtgRqV0PLkyjT8R9znpC9ECGurgfelIhn_q5BwgThg6SpdfE2R30obAAaawk0FIGLlBowjt_/pub?gid=1824827366&single=true&output=csv' },
                        { id: 'vendas', label: 'Vendas', url: 'https://docs.google.com/spreadsheets/d/e/2PACX-1vR9lG9sbtgRqV0PLkyjT8R9znpC9ECGurgfelIhn_q5BwgThg6SpdfE2R30obAAaawk0FIGLlBowjt_/pub?gid=959742330&single=true&output=csv' },
                        { id: 'cobertura', label: 'Cobertura', url: 'https://docs.google.com/spreadsheets/d/e/2PACX-1vR9lG9sbtgRqV0PLkyjT8R9znpC9ECGurgfelIhn_q5BwgThg6SpdfE2R30obAAaawk0FIGLlBowjt_/pub?gid=1824827366&single=true&output=csv' },
                        { id: 'nao_cobertos_clientes', label: 'N√£o Cobertos (Cliente)', url: 'https://docs.google.com/spreadsheets/d/e/2PACX-1vR9lG9sbtgRqV0PLkyjT8R9znpC9ECGurgfelIhn_q5BwgThg6SpdfE2R30obAAaawk0FIGLlBowjt_/pub?gid=953923858&single=true&output=csv' },
                        { id: 'nao_cobertos_fornecedor', label: 'N√£o Cobertos (Fornecedor)', url: 'https://docs.google.com/spreadsheets/d/e/2PACX-1vR9lG9sbtgRqV0PLkyjT8R9znpC9ECGurgfelIhn_q5BwgThg6SpdfE2R30obAAaawk0FIGLlBowjt_/pub?gid=1981950621&single=true&output=csv' },
                        { id: 'msl_danone', label: 'MSL DANONE', url: 'https://docs.google.com/spreadsheets/d/e/2PACX-1vR9lG9sbtgRqV0PLkyjT8R9znpC9ECGurgfelIhn_q5BwgThg6SpdfE2R30obAAaawk0FIGLlBowjt_/pub?gid=402511992&single=true&output=csv' },
                        { id: 'msl_otg', label: 'MSL OTG', url: 'https://docs.google.com/spreadsheets/d/e/2PACX-1vR9lG9sbtgRqV0PLkyjT8R9znpC9ECGurgfelIhn_q5BwgThg6SpdfE2R30obAAaawk0FIGLlBowjt_/pub?gid=1571578249&single=true&output=csv' },
                        { id: 'msl_mini', label: 'MSL MINI', url: 'https://docs.google.com/spreadsheets/d/e/2PACX-1vR9lG9sbtgRqV0PLkyjT8R9znpC9ECGurgfelIhn_q5BwgThg6SpdfE2R30obAAaawk0FIGLlBowjt_/pub?gid=544996255&single=true&output=csv' },
