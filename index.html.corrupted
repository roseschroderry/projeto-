        async function respondAboutFiles(message) {
            if (!db) {
                return `O banco de dados ainda est√° inicializando. Por favor, aguarde um momento e tente novamente.`;
            }

            try {
                const files = await getAllFilesFromDB();
                
                if (files.length === 0) {
                    return `Voc√™ ainda n√£o enviou nenhum arquivo. V√° para a se√ß√£o <strong>Arquivos IA</strong> e envie documentos para que eu possa analis√°-los! üìÅ`;
                }

                // Estat√≠sticas dos arquivos
                const totalSize = files.reduce((sum, f) => sum + f.size, 0);
                const categories = {};
                files.forEach(f => {
                    const cat = f.analysis?.category || 'Outros';
                    categories[cat] = (categories[cat] || 0) + 1;
                });

                let response = `üìä <strong>An√°lise dos seus arquivos:</strong>\n\n`;
                response += `üìÅ Total: <strong>${files.length} arquivo(s)</strong>\n`;
                response += `üíæ Tamanho total: <strong>${(totalSize / 1024 / 1024).toFixed(2)} MB</strong>\n\n`;
                
                response += `<strong>Por categoria:</strong>\n`;
                Object.entries(categories).forEach(([cat, count]) => {
                    response += `${cat}: ${count} arquivo(s)\n`;
                });

                // Arquivos recentes
                const recentFiles = files.slice(-3).reverse();
                response += `\n<strong>√öltimos enviados:</strong>\n`;
                recentFiles.forEach(f => {
                    response += `‚Ä¢ ${f.name} (${(f.size / 1024).toFixed(2)} KB)\n`;
                });

                // Insights espec√≠ficos se perguntarem
                if (message.includes('pdf')) {
                    const pdfs = files.filter(f => f.type === 'pdf');
                    response += `\nüìÑ Voc√™ tem <strong>${pdfs.length} arquivo(s) PDF</strong>`;
                }
                if (message.includes('image') || message.includes('imagem')) {
                    const images = files.filter(f => ['png', 'jpg', 'jpeg', 'gif'].includes(f.type));
                    response += `\nüñºÔ∏è Voc√™ tem <strong>${images.length} imagem(ns)</strong>`;
