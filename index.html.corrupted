    <script>
        console.log('üöÄ P√ÅGINA CARREGADA');
        
        // ========== VARI√ÅVEIS GLOBAIS ==========
        window.currentUser = null;
        window.authToken = null;
        
        // ========== FUN√á√ïES CR√çTICAS NO TOPO PARA ONCLICK ==========
        
        // LOGIN
        function doLogin() {
            console.log('üîê Login iniciado');
            
            try {
                const email = document.getElementById('loginEmail').value.trim();
                const password = document.getElementById('loginPassword').value;
                const rememberMe = document.getElementById('rememberMe')?.checked || false;
                // Usar Backend API por padr√£o se n√£o houver configura√ß√£o
                const savedBackendApi = localStorage.getItem('useBackendApi');
                const useBackend = savedBackendApi === null ? true : savedBackendApi === 'true';
                const btn = document.getElementById('loginBtn');
                
                if (!email || !password) {
                    alert('Preencha email e senha!');
                    return;
                }
                
                btn.disabled = true;
                btn.innerHTML = 'Entrando...';
                
                const users = JSON.parse(localStorage.getItem('registeredUsers') || '[]');
                const user = users.find(u => u.email === email && u.password === password);
                
                if (user) {
                    window.currentUser = user;
                    window.authToken = 'token_' + Date.now();
                    
                    const storage = rememberMe ? localStorage : sessionStorage;
                    storage.setItem('token', window.authToken);
                    storage.setItem('user', JSON.stringify(user));
                    
                    document.getElementById('authPage').style.display = 'none';
                    document.getElementById('mainApp').style.display = 'flex';
                    
                    setTimeout(() => {
                        const userName = document.getElementById('userName');
                        const userAvatar = document.getElementById('userAvatar');
                        if (userName) userName.textContent = user.name;
                        if (userAvatar) userAvatar.textContent = user.name.charAt(0).toUpperCase();
                        
                        if (user.role === 'admin') {
                            const adminSection = document.getElementById('adminSection');
                            if (adminSection) {
                                adminSection.style.display = 'block';
                                console.log('‚úÖ MENU ADMIN ATIVADO!');
                            }
                        }
                        
                        // Carregar dados do Google Sheets
                        setTimeout(() => window.carregarDadosDoSheets(), 1000);
                    }, 100);
                    
                    console.log('‚úÖ Login OK');
                } else {
                    alert('Email ou senha incorretos!');
                    btn.disabled = false;
                    btn.innerHTML = 'Entrar';
                }
            } catch (error) {
                console.error('‚ùå Erro no login:', error);
