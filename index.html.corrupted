            modal.innerHTML = tableHTML;
            document.body.appendChild(modal);
            
            // Adicionar fun√ß√£o de filtro
            window.filterReportData = function(searchTerm) {
                const term = searchTerm.toLowerCase().trim();
                const isMobile = window.innerWidth <= 768;
                let visibleCount = 0;
                
                if (isMobile) {
                    // Filtrar cards
                    const cards = document.querySelectorAll('.report-card');
                    cards.forEach(card => {
                        const values = Array.from(card.querySelectorAll('.card-value'))
                            .map(el => el.textContent.toLowerCase())
                            .join(' ');
                        
                        if (term === '' || values.includes(term)) {
                            card.style.display = 'block';
                            visibleCount++;
                        } else {
                            card.style.display = 'none';
                        }
                    });
                } else {
                    // Filtrar linhas da tabela
                    const rows = document.querySelectorAll('.report-row');
                    rows.forEach(row => {
                        const cells = Array.from(row.querySelectorAll('.table-cell'))
                            .map(cell => cell.textContent.toLowerCase())
                            .join(' ');
                        
                        if (term === '' || cells.includes(term)) {
                            row.style.display = '';
                            visibleCount++;
                        } else {
                            row.style.display = 'none';
                        }
                    });
                }
                
                // Atualizar contador
                const filteredCount = document.getElementById('filteredCount');
                const totalCount = document.getElementById('totalCount');
                if (filteredCount) {
                    filteredCount.textContent = visibleCount;
                    if (term && totalCount) {
                        totalCount.style.display = 'inline';
                    } else if (totalCount) {
                        totalCount.style.display = 'none';
                    }
                }
            };
            
            console.log(`‚úÖ Modal de relat√≥rio "${reportId}" aberto com ${reportData.length} registros`);
        };
        
        // Fun√ß√£o para fechar modal de relat√≥rio
        window.closeReportModal = function() {
            const modal = document.getElementById('reportModal');
            if (modal) {
                modal.remove();
            }
        };
        
        // Fun√ß√£o para exportar relat√≥rio como CSV
        window.exportReportToCSV = function(reportId) {
            if (!window.reportDataCache || !window.reportDataCache[reportId]) {
                alert('Erro: Dados do relat√≥rio n√£o encontrados');
                return;
            }
            
            const data = window.reportDataCache[reportId];
            if (data.length === 0) {
                alert('Nenhum dado para exportar');
                return;
            }
            
            // Criar CSV
            const headers = Object.keys(data[0]);
            let csv = headers.join(',') + '\n';
            
            data.forEach(row => {
                const values = headers.map(h => {
                    const value = String(row[h] || '');
                    // Escapar v√≠rgulas e aspas
                    if (value.includes(',') || value.includes('"') || value.includes('\n')) {
                        return '"' + value.replace(/"/g, '""') + '"';
                    }
                    return value;
                });
                csv += values.join(',') + '\n';
            });
            
            // Download
            const blob = new Blob([csv], { type: 'text/csv;charset=utf-8;' });
            const link = document.createElement('a');
            const url = URL.createObjectURL(blob);
            link.setAttribute('href', url);
            link.setAttribute('download', `${reportId}_${new Date().toISOString().split('T')[0]}.csv`);
            link.style.visibility = 'hidden';
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
            
            console.log(`‚úÖ Relat√≥rio "${reportId}" exportado com ${data.length} registros`);
        };
        
        // RELAT√ìRIOS - Abrir (wrapper para compatibilidade)
        window.openReport = function(reportId) {
