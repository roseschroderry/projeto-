        // Remover duplicidades
        function removeDuplicates(data, keys) {
            const seen = new Set();
            return data.filter(row => {
                const key = keys.map(k => row[k] || row[k.replace(' do ', '')] || row[k.replace('√≥', 'o')] || '').join('|');
                if (seen.has(key)) return false;
                seen.add(key);
                return true;
            });
        }

