        // NAVEGA√á√ÉO
        window.navigateTo = function(e, section) {
            try {
                if (e) e.preventDefault();

                // Verificar permiss√µes
                if ((section === 'dashboard' || section === 'arquivos' || section === 'usuarios') && 
                    (!window.currentUser || window.currentUser.role !== 'admin')) {
                    alert('‚õî Acesso restrito aos administradores');
                    return;
                }

                // Atualizar menu ativo
                document.querySelectorAll('.sidebar-link').forEach(link => {
                    link.classList.remove('active');
                });
                if (e && e.currentTarget) {
                    e.currentTarget.classList.add('active');
                }

                // Trocar se√ß√µes
                document.querySelectorAll('.content-section').forEach(sec => {
                    sec.classList.add('hidden');
                });

                const targetSection = document.getElementById(section + 'Section');
                if (targetSection) {
                    targetSection.classList.remove('hidden');
                    console.log('‚úÖ Navegou para:', section);
                    
                    // Reinicializar dashboard quando navegar para ele
                    if (section === 'dashboard' && window.adminDashboard) {
                        setTimeout(() => {
                            window.adminDashboard.init();
                        }, 100);
                    }
                    
                    // Carregar lista de usu√°rios quando navegar para usu√°rios
                    if (section === 'usuarios' && typeof loadUsersList === 'function') {
                        setTimeout(() => {
                            loadUsersList();
                        }, 100);
                    }
                } else {
                    alert('‚ùå Se√ß√£o n√£o encontrada: ' + section);
                }
                
                closeMobileMenu();
            } catch (error) {
                console.error('‚ùå Erro navega√ß√£o:', error);
                alert('Erro: ' + error.message);
            }
        };
        
