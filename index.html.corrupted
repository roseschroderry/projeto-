        // IDENTIFICA√á√ÉO AUTOM√ÅTICA DE COLUNA MSL (Adaptado do Python)
        // =================================================================================
        
        function identificarColunaMSL(data) {
            console.log('üîç Identificando coluna MSL automaticamente...');
            
            const valoresMSL = ['MSL MINI', 'MSL SUPER', 'MSL OTG', 'EXP'];
            
            if (!data || data.length === 0) return null;
            
            const primeiraLinha = data[0];
            const colunas = Object.keys(primeiraLinha);
            
            for (const coluna of colunas) {
                // Verificar se alguma linha tem valores MSL nesta coluna
                for (const row of data.slice(0, 100)) { // Verificar primeiras 100 linhas
                    const valor = String(row[coluna] || '').toUpperCase().trim();
                    
                    if (valoresMSL.some(msl => valor.includes(msl))) {
                        console.log(`‚úÖ Coluna MSL identificada: "${coluna}"`);
                        return coluna;
                    }
                }
            }
            
            console.log('‚ö†Ô∏è Coluna MSL n√£o encontrada, usando nome padr√£o');
            return 'ATIVIDADE'; // Fallback
        }
        
        // =================================================================================
        // FILTROS MSL (Adaptado do Python)
        // =================================================================================
        
        function filtrarMSL(data, codvd, vendedor, tipoMSL) {
            console.log(`üîç Filtrando MSL ${tipoMSL} para CODVD=${codvd}, VENDEDOR=${vendedor || 'TODOS'}`);
            
            const colunaMSL = identificarColunaMSL(data);
            
            if (!colunaMSL) {
                console.error('‚ùå Coluna MSL n√£o identificada!');
                return [];
            }
            
            const filtered = data.filter(row => {
                const rowCodvd = String(row['CODVD'] || row['C√≥digo do Vendedor'] || row['Codigo Vendedor'] || '').trim();
                const rowVendedor = String(row['VENDEDOR'] || row['Nome do Vendedor'] || row['Vendedor'] || '').trim().toUpperCase();
                const valorMSL = String(row[colunaMSL] || '').trim().toUpperCase();
                const status = String(row['STATUS'] || row['Status'] || '').trim().toUpperCase();
                
                const codvdMatch = rowCodvd === codvd;
                const vendedorMatch = !vendedor || rowVendedor.includes(vendedor.toUpperCase());
                
                return codvdMatch && vendedorMatch &&
                       valorMSL === tipoMSL.toUpperCase() &&
                       (status === 'OK' || status === 'FALTA');
            });
            
            console.log(`‚úÖ Encontrados ${filtered.length} registros`);
            
            const result = filtered.map(row => ({
                'CODVD': String(row['CODVD'] || row['C√≥digo do Vendedor'] || '').trim(),
                'VENDEDOR': String(row['VENDEDOR'] || row['Nome do Vendedor'] || row['Vendedor'] || '').trim(),
                'CODCLI': String(row['CODCLI'] || row['C√≥digo do Cliente'] || '').trim(),
                'CLIENTE': String(row['CLIENTE'] || row['Cliente'] || '').trim(),
                'STATUS': String(row['STATUS'] || row['Status'] || '').trim()
            }));
            
            return removeDuplicates(result, ['CODCLI']);
        }
        
