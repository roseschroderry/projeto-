        // AI File Analysis
        function analyzeFileWithAI(file) {
            const fileType = getFileType(file.name);
            const size = (file.size / 1024).toFixed(2);
            
            let analysis = {
                type: fileType,
                category: categorizeFile(fileType),
                size: size + ' KB',
                insights: [],
                tags: []
            };
            
            // Categoriza√ß√£o inteligente
            if (['pdf', 'doc', 'docx', 'txt', 'md'].includes(fileType)) {
                analysis.category = 'üìÑ Documento';
                analysis.insights.push('Documento de texto identificado');
                analysis.insights.push('Recomendado para leitura e an√°lise de conte√∫do');
                analysis.tags = ['documento', 'texto', 'leitura'];
            } else if (['xls', 'xlsx', 'csv'].includes(fileType)) {
                analysis.category = 'üìä Planilha';
                analysis.insights.push('Planilha de dados detectada');
                analysis.insights.push('Cont√©m dados estruturados para an√°lise');
                analysis.tags = ['dados', 'planilha', 'an√°lise'];
            } else if (['png', 'jpg', 'jpeg', 'gif'].includes(fileType)) {
                analysis.category = 'üñºÔ∏è Imagem';
                analysis.insights.push('Arquivo de imagem identificado');
                analysis.insights.push('Formato: ' + fileType.toUpperCase());
                analysis.tags = ['imagem', 'visual', 'm√≠dia'];
            } else if (['json', 'xml'].includes(fileType)) {
                analysis.category = '‚öôÔ∏è Dados';
                analysis.insights.push('Arquivo de dados estruturados');
                analysis.insights.push('Formato de configura√ß√£o ou API');
                analysis.tags = ['dados', 'estruturado', 'config'];
            } else if (fileType === 'zip') {
                analysis.category = 'üì¶ Arquivo';
                analysis.insights.push('Arquivo compactado detectado');
                analysis.insights.push('Cont√©m m√∫ltiplos arquivos');
                analysis.tags = ['compactado', 'm√∫ltiplos', 'arquivo'];
            }
            
            // An√°lise de tamanho
            if (file.size < 1024 * 100) {
                analysis.insights.push('Arquivo pequeno - processamento r√°pido');
            } else if (file.size < 1024 * 1024 * 5) {
                analysis.insights.push('Arquivo m√©dio - tempo de processamento moderado');
            } else {
                analysis.insights.push('Arquivo grande - pode requerer mais tempo');
            }
            
            return analysis;
        }
        
        function getFileType(filename) {
            const ext = filename.split('.').pop().toLowerCase();
            return ext;
        }
        
        function categorizeFile(type) {
            const categories = {
                'pdf': 'üìÑ Documento',
                'doc': 'üìÑ Documento',
                'docx': 'üìÑ Documento',
                'txt': 'üìÑ Documento',
                'md': 'üìÑ Documento',
                'xls': 'üìä Planilha',
                'xlsx': 'üìä Planilha',
                'csv': 'üìä Planilha',
                'png': 'üñºÔ∏è Imagem',
                'jpg': 'üñºÔ∏è Imagem',
                'jpeg': 'üñºÔ∏è Imagem',
                'gif': 'üñºÔ∏è Imagem',
                'json': '‚öôÔ∏è Dados',
                'xml': '‚öôÔ∏è Dados',
                'zip': 'üì¶ Arquivo'
            };
            return categories[type] || 'üìÑ Arquivo';
        }
        
        function updateAnalytics() {
            if (!db) return;
            
            const transaction = db.transaction(['files'], 'readonly');
            const store = transaction.objectStore('files');
            const request = store.getAll();
            
            request.onsuccess = () => {
                const files = request.result;
                
                // Total de arquivos
                document.getElementById('totalFilesCount').textContent = files.length;
                document.getElementById('fileCount').textContent = files.length;
                document.getElementById('aiAnalysisCount').textContent = files.length;
                
                // Tipos de arquivo
                const types = new Set(files.map(f => f.type));
                document.getElementById('fileTypesCount').textContent = types.size;
                
                // Tamanho total
                const totalSize = files.reduce((sum, f) => sum + f.size, 0);
                document.getElementById('totalFileSize').textContent = (totalSize / 1024 / 1024).toFixed(2) + ' MB';
                
                // Distribui√ß√£o por tipo
                const distribution = {};
                files.forEach(f => {
                    const category = categorizeFile(f.type);
                    distribution[category] = (distribution[category] || 0) + 1;
                });
                
                let distHtml = '<div style="display: flex; flex-direction: column; gap: 12px;">';
                Object.entries(distribution).forEach(([category, count]) => {
                    const percentage = ((count / files.length) * 100).toFixed(1);
                    distHtml += `
                        <div>
                            <div style="display: flex; justify-content: space-between; margin-bottom: 6px;">
                                <span style="font-weight: 600;">${category}</span>
                                <span style="color: var(--text-secondary);">${count} (${percentage}%)</span>
                            </div>
                            <div style="background: #E5E7EB; border-radius: 8px; height: 8px; overflow: hidden;">
                                <div style="background: linear-gradient(90deg, #DC2626, #EF4444); height: 100%; width: ${percentage}%;"></div>
                            </div>
                        </div>
                    `;
                });
                distHtml += '</div>';
                document.getElementById('fileDistribution').innerHTML = distHtml;
                
                // Insights da IA
                let insightsHtml = '<div style="display: flex; flex-direction: column; gap: 12px;">';
                if (files.length > 0) {
                    const mostCommon = Object.entries(distribution).sort((a, b) => b[1] - a[1])[0];
                    insightsHtml += `
                        <div style="padding: 16px; background: rgba(220, 38, 38, 0.05); border-radius: 8px; border-left: 4px solid #DC2626;">
                            <strong>üéØ Tipo mais comum:</strong> ${mostCommon[0]} (${mostCommon[1]} arquivos)
                        </div>
                    `;
                    
                    if (totalSize > 10 * 1024 * 1024) {
                        insightsHtml += `
                            <div style="padding: 16px; background: rgba(245, 158, 11, 0.05); border-radius: 8px; border-left: 4px solid #F59E0B;">
                                <strong>‚ö†Ô∏è Alerta:</strong> Armazenamento acima de 10MB - considere fazer backup
                            </div>
                        `;
                    }
                    
                    insightsHtml += `
                        <div style="padding: 16px; background: rgba(16, 185, 129, 0.05); border-radius: 8px; border-left: 4px solid #10B981;">
                            <strong>‚úÖ Organiza√ß√£o:</strong> Arquivos bem categorizados em ${types.size} formatos diferentes
                        </div>
                    `;
                } else {
                    insightsHtml += '<p style="text-align: center; color: var(--text-secondary); padding: 20px;">Nenhum arquivo analisado ainda</p>';
                }
                insightsHtml += '</div>';
                document.getElementById('aiInsights').innerHTML = insightsHtml;
            };
        }

        function handleFileUpload(event) {
            const files = event.target.files;
            const container = document.getElementById('filesListContainer');
            const analysisResult = document.getElementById('fileAnalysisResult');
            const analysisContent = document.getElementById('analysisContent');
            
            if (files.length === 0) return;

            let analysisHtml = '';
            
            Array.from(files).forEach(file => {
                // An√°lise IA do arquivo
                const analysis = analyzeFileWithAI(file);
                
                // Salvar no banco de dados
                saveFileToDatabase(file, analysis);
                
                // Adicionar √† an√°lise
                analysisHtml += `
                    <div style="margin-bottom: 16px; padding-bottom: 16px; border-bottom: 1px solid rgba(220, 38, 38, 0.2);">
                        <div style="font-weight: 700; margin-bottom: 8px;">üìÑ ${file.name}</div>
                        <ul style="margin: 0; padding-left: 20px; color: var(--text-dark);">
                            ${analysis.insights.map(insight => `<li style="margin-bottom: 4px;">${insight}</li>`).join('')}
                        </ul>
                    </div>
                `;
            });

            // Mostrar an√°lise IA
            analysisContent.innerHTML = analysisHtml;
            analysisResult.style.display = 'block';
            
            const isAdmin = currentUser.role === 'admin';
            const shareMsg = isAdmin ? ' e compartilhado(s) com todos os usu√°rios' : '';
            showNotification(`${files.length} arquivo(s) analisado(s) pela IA${shareMsg}!`, 'success');
            
            // Reset input
            event.target.value = '';
            
            // Recarregar lista ap√≥s salvar
            setTimeout(() => loadUserFiles(), 500);
        }

        // Lista de usu√°rios
        function loadUsersList() {
            const users = JSON.parse(localStorage.getItem('registeredUsers') || '[]');
            const container = document.getElementById('usersList');
            
            if (users.length === 0) {
                container.innerHTML = '<p style="text-align: center; color: var(--text-secondary); padding: 20px;">Nenhum usu√°rio cadastrado</p>';
                return;
            }

            let html = '<table class="stats-table"><thead><tr><th>Nome</th><th>Email</th><th>Tipo</th><th>Data</th></tr></thead><tbody>';

            users.forEach(user => {
                const roleText = user.role === 'admin' ? 'üëë Admin' : 'üë§ Usu√°rio';
                const date = new Date(user.createdAt).toLocaleDateString('pt-BR');
                
                html += `
                    <tr>
                        <td><strong>${user.name}</strong></td>
                        <td>${user.email}</td>
                        <td>${roleText}</td>
                        <td style="font-size: 13px; color: var(--text-secondary);">${date}</td>
                    </tr>
                `;
            });

            html += '</tbody></table>';
            container.innerHTML = html;
        }

