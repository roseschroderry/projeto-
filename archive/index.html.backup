        // Fun√ß√£o para exportar relat√≥rio
        function exportReport(reportName) {
            const sheets = JSON.parse(localStorage.getItem('googleSheetsData') || '[]');
            const report = sheets.find(s => s.id === reportName);
            
            if (report && report.data) {
                const csv = [Object.keys(report.data[0]).join(',')];
                report.data.forEach(row => {
                    csv.push(Object.values(row).map(v => `"${v}"`).join(','));
                });
                
                const blob = new Blob([csv.join('\n')], { type: 'text/csv' });
                const url = URL.createObjectURL(blob);
                const a = document.createElement('a');
                a.href = url;
                a.download = `${reportName}_${new Date().toISOString().split('T')[0]}.csv`;
                a.click();
                URL.revokeObjectURL(url);
                
                alert(`‚úÖ Relat√≥rio exportado com sucesso!\n\nArquivo: ${reportName}.csv`);
            }
        }
        
