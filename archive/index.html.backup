        function identificarColunaMSL(data) {
            console.log('üîç Identificando coluna MSL automaticamente...');
            
            const valoresMSL = ['MSL MINI', 'MSL SUPER', 'MSL OTG', 'EXP'];
            
            if (!data || data.length === 0) return null;
            
            const primeiraLinha = data[0];
            const colunas = Object.keys(primeiraLinha);
            
            for (const coluna of colunas) {
                // Verificar se alguma linha tem valores MSL nesta coluna
                for (const row of data.slice(0, 100)) { // Verificar primeiras 100 linhas
                    const valor = String(row[coluna] || '').toUpperCase().trim();
                    
                    if (valoresMSL.some(msl => valor.includes(msl))) {
                        console.log(`‚úÖ Coluna MSL identificada: "${coluna}"`);
                        return coluna;
                    }
                }
            }
            
            console.log('‚ö†Ô∏è Coluna MSL n√£o encontrada, usando nome padr√£o');
            return 'ATIVIDADE'; // Fallback
        }
        
