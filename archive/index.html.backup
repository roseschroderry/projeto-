            const vendorName = chatContext.vendorName;
            
            const filtered = data.filter(row => {
                const rowVendorCode = String(row['C√≥digo do Vendedor'] || row['Codigo do Vendedor'] || row['CodigoVendedor'] || '').trim();
                const rowVendorName = String(row['Nome do Vendedor'] || row['NomeVendedor'] || row['Vendedor'] || '').trim().toLowerCase();
                const categoria = String(row['Categoria do Produto'] || row['Categoria'] || row['Produto'] || '').trim().toLowerCase();
                const status = String(row['Status'] || '').trim().toUpperCase();
                
                return rowVendorCode === vendorCode &&
                       rowVendorName.includes(vendorName.toLowerCase()) &&
                       categoria.includes('danone') &&
                       (status === 'FALTA' || status === 'OK');
            });
            
            return removeDuplicates(filtered, ['C√≥digo do Cliente', 'Categoria do Produto']);
        }

        // 7. Novos Clientes
        function filterNovosClientes(data) {
            const filtered = data.filter(row => {
                const isNovo = String(row['Novo Cliente'] || row['NovoCliente'] || row['Novo'] || '').trim().toLowerCase();
                return isNovo === 'sim' || isNovo === 's' || isNovo === 'true' || isNovo === '1';
            });
            
            return removeDuplicates(filtered, ['C√≥digo do Cliente']);
        }

        // 8. Queijo do Reino
        function filterQueijoReino(data) {
            const filtered = data.filter(row => {
                const categoria = String(row['Categoria'] || row['Produto'] || row['Categoria do Produto'] || '').trim().toLowerCase();
                return categoria.includes('queijo') && categoria.includes('reino');
            });
            
            return removeDuplicates(filtered, ['C√≥digo do Cliente']);
        }

        // Remover duplicidades
        function removeDuplicates(data, keys) {
            const seen = new Set();
            return data.filter(row => {
                const key = keys.map(k => row[k] || row[k.replace(' do ', '')] || row[k.replace('√≥', 'o')] || '').join('|');
                if (seen.has(key)) return false;
                seen.add(key);
                return true;
            });
        }

