            }

            const totalSize = files.reduce((sum, f) => sum + f.size, 0);
            const types = new Set(files.map(f => f.type));
            
            return `üìä <strong>Estat√≠sticas:</strong>\n\n` +
                   `üìÅ Arquivos: <strong>${files.length}</strong>\n` +
                   `üìÇ Tipos diferentes: <strong>${types.size}</strong>\n` +
                   `üíæ Espa√ßo usado: <strong>${(totalSize / 1024 / 1024).toFixed(2)} MB</strong>\n` +
                   `üìÖ √öltimo upload: <strong>${new Date(files[files.length - 1]?.uploadDate).toLocaleDateString('pt-BR')}</strong>`;
        }

