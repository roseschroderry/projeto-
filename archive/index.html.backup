                return [];
            }
            
            const filtered = data.filter(row => {
                const rowCodvd = String(row['CODVD'] || row['C√≥digo do Vendedor'] || row['Codigo Vendedor'] || '').trim();
                const rowVendedor = String(row['VENDEDOR'] || row['Nome do Vendedor'] || row['Vendedor'] || '').trim().toUpperCase();
                const valorMSL = String(row[colunaMSL] || '').trim().toUpperCase();
                const status = String(row['STATUS'] || row['Status'] || '').trim().toUpperCase();
                
                const codvdMatch = rowCodvd === codvd;
                const vendedorMatch = !vendedor || rowVendedor.includes(vendedor.toUpperCase());
                
                return codvdMatch && vendedorMatch &&
                       valorMSL === tipoMSL.toUpperCase() &&
