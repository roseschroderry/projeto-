        async function generateIntelligentResponse(message) {
            const lowerMessage = message.toLowerCase();
            
            // =================================================================================
            // M√ÅQUINA DE ESTADOS (Adaptado do Python ChatIA)
            // =================================================================================
            
            // ESTADO: INICIAL - Aguardando comando "relat√≥rios"
            if (chatContext.estado === 'INICIAL') {
                if (lowerMessage.includes('relat√≥rio') || lowerMessage.includes('relatorio') || lowerMessage === 'relat√≥rios' || lowerMessage === 'relatorios') {
                    chatContext.estado = 'ESCOLHA';
                    return showReportsMenu();
                }
                
                // Respostas gerais quando n√£o est√° em fluxo de relat√≥rio
                return await handleGeneralQuestions(lowerMessage);
            }
            
            // ESTADO: ESCOLHA - Aguardando escolha do relat√≥rio (Adaptado do Python)
            if (chatContext.estado === 'ESCOLHA') {
                const reportChoices = {
                    '1': 'nao_cobertos_clientes',
                    '2': 'nao_cobertos_fornecedor',
                    '3': 'msl_super',
                    '4': 'msl_mini',
                    '5': 'msl_otg',
                    '6': 'exp'
                };
                
                const reportNames = {
                    'n√£o cobertos clientes': 'nao_cobertos_clientes',
                    'nao cobertos clientes': 'nao_cobertos_clientes',
                    'cobertos clientes': 'nao_cobertos_clientes',
                    'n√£o cobertos fornecedor': 'nao_cobertos_fornecedor',
                    'nao cobertos fornecedor': 'nao_cobertos_fornecedor',
                    'cobertos fornecedor': 'nao_cobertos_fornecedor',
                    'msl super': 'msl_super',
                    'super': 'msl_super',
                    'msl mini': 'msl_mini',
                    'mini': 'msl_mini',
                    'msl otg': 'msl_otg',
                    'otg': 'msl_otg',
                    'exp': 'exp'
                };
                
                // Verificar escolha por n√∫mero
                if (reportChoices[lowerMessage]) {
                    chatContext.activeReport = reportChoices[lowerMessage];
                } else {
                    // Verificar escolha por nome
                    let found = false;
                    for (const [key, value] of Object.entries(reportNames)) {
                        if (lowerMessage.includes(key)) {
                            chatContext.activeReport = value;
                            found = true;
                            break;
                        }
                    }
                    if (!found) {
                        return `‚ùå Op√ß√£o inv√°lida.\n\nEscolha uma op√ß√£o:\n` +
                               `1 - N√£o cobertos clientes\n` +
                               `2 - N√£o cobertos fornecedor\n` +
                               `3 - MSL Super\n` +
                               `4 - MSL Mini\n` +
                               `5 - MSL OTG\n` +
                               `6 - EXP`;
                    }
                }
                
                // TODOS os relat√≥rios agora precisam de CODVD e VENDEDOR
                chatContext.estado = 'CODVD';
                return `‚úÖ Relat√≥rio selecionado: <strong>${getReportLabel(chatContext.activeReport)}</strong>\n\n` +
                       `üìù Agora informe o <strong>c√≥digo do vendedor (CODVD)</strong>:`;
            }
            
            // ESTADO: CODVD - Aguardando c√≥digo do vendedor
            if (chatContext.estado === 'CODVD') {
                const codigo = message.trim();
                if (!codigo || codigo.length === 0) {
                    return `‚ùå Por favor, informe um c√≥digo v√°lido.`;
                }
                
                chatContext.codvd = codigo;
                chatContext.estado = 'VENDEDOR';
                return `‚úÖ C√≥digo registrado: <strong>${codigo}</strong>\n\n` +
                       `üìù Informe o <strong>nome do vendedor</strong> ou digite <strong>"todos"</strong> para buscar todos os vendedores deste c√≥digo:`;
            }
            
            // ESTADO: VENDEDOR - Aguardando nome do vendedor e processando
            if (chatContext.estado === 'VENDEDOR') {
                const input = message.trim().toLowerCase();
                
                // Verificar se quer buscar todos os vendedores
                if (input === 'todos' || input === 'todo' || input === 'pular' || input.length === 0) {
                    chatContext.vendedor = null; // null = buscar todos
                } else {
                    chatContext.vendedor = message.trim();
                }
                
                console.log(`üîç Processando: CODVD=${chatContext.codvd}, VENDEDOR=${chatContext.vendedor || 'TODOS'}, RELAT√ìRIO=${chatContext.activeReport}`);
                
                // Processar o relat√≥rio com os filtros (agora inclui MSL)
                const resultado = await processarRelatorioComFiltros(
                    chatContext.activeReport,
                    chatContext.codvd,
