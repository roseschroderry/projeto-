        function loadDatabaseTable() {
            const table = document.getElementById('dbTableSelect').value;
            const container = document.getElementById('databaseTableContent');
            
            if (!db) {
                container.innerHTML = '<p>Banco de dados n√£o inicializado</p>';
                return;
            }
            
            const transaction = db.transaction([table], 'readonly');
            const store = transaction.objectStore(table);
            const request = store.getAll();
            
            request.onsuccess = () => {
                const data = request.result;
                
                if (data.length === 0) {
                    container.innerHTML = '<p style="text-align: center; color: var(--text-secondary); padding: 20px;">Nenhum registro encontrado</p>';
                    return;
                }
                
                let html = '<table class="stats-table"><thead><tr>';
                
                const keys = Object.keys(data[0]).filter(k => k !== 'id');
                keys.forEach(key => {
                    html += `<th>${key.charAt(0).toUpperCase() + key.slice(1)}</th>`;
                });
                html += '</tr></thead><tbody>';
                
                data.forEach(row => {
                    html += '<tr>';
                    keys.forEach(key => {
                        let value = row[key];
                        if (key.includes('Date') || key === 'timestamp') {
                            value = new Date(value).toLocaleString('pt-BR');
                        }
                        if (typeof value === 'object') {
                            value = JSON.stringify(value);
                        }
                        html += `<td>${value}</td>`;
                    });
                    html += '</tr>';
                });
                
                html += '</tbody></table>';
                container.innerHTML = html;
            };
        }
        
