            }
        ];

        // Vari√°vel onde os dados ficam salvos na mem√≥ria
        var reportDataCache = {};
        
        // Carregar dados do localStorage se existir
        try {
            const savedData = localStorage.getItem('googleSheetsCache');
            if (savedData) {
                reportDataCache = JSON.parse(savedData);
                console.log('üì¶ Dados carregados do cache local:', Object.keys(reportDataCache).length, 'relat√≥rios');
            }
        } catch (e) {
            console.warn('‚ö†Ô∏è Erro ao carregar cache:', e);
        }

        // =================================================================================
        // 2. FUN√á√ÉO UTILIT√ÅRIA: PARSER DE CSV (Converte texto em objeto JSON)
        // =================================================================================
        function parseRobustCSV(text) {
            const rows = text.trim().replace(/^\uFEFF/, '').match(/(?:[^\r\n"]+|"[^"]*")+/g);
            if (!rows || rows.length < 2) return [];
            
            const headers = rows[0].split(',').map(h => h.trim());
            const regex = /("([^"]|"")"|[^,])(,|$)/g;
            const res = [];
            
            for (let i = 1; i < rows.length; i++) {
                if (!rows[i].trim()) continue;
                const obj = {}; let m; let j = 0; regex.lastIndex = 0;
                while ((m = regex.exec(rows[i])) !== null) {
                    if (j >= headers.length) break;
                    let v = m[1];
                    if (v.startsWith('"') && v.endsWith('"')) v = v.substring(1, v.length - 1).replace(/""/g, '"');
                    obj[headers[j]] = v.trim(); j++;
                    if (m.index === regex.lastIndex) regex.lastIndex++;
                }
                res.push(obj);
            }
            return res;
        }

        // =================================================================================
        // 3. FUN√á√ÉO PRINCIPAL DE REQUISI√á√ÉO (FETCH)
        // =================================================================================
        async function carregarDadosDoSheets() {
            console.log("üì• Iniciando download das planilhas do Google Sheets...");
            
            // Mostrar indicador de carregamento no chat
            const chatMessages = document.getElementById('chatMessages');
            if (chatMessages) {
                const loadingMsg = document.createElement('div');
                loadingMsg.className = 'message bot-message';
                loadingMsg.id = 'loading-reports-message';
                loadingMsg.innerHTML = `
                    <div class="message-content">
                        <div class="message-avatar">ü§ñ</div>
                        <div class="message-text">
                            <div style="display: flex; align-items: center; gap: 10px;">
                                <div class="loading" style="width: 20px; height: 20px;"></div>
                                <span>Carregando relat√≥rios do Google Sheets...</span>
                            </div>
                        </div>
                    </div>
                `;
                chatMessages.appendChild(loadingMsg);
                chatMessages.scrollTop = chatMessages.scrollHeight;
            }
            
            const promises = REPORTS_CONFIG.map(async (config) => {
                try {
                    // cache: "no-store" garante que n√£o pegaremos uma vers√£o velha em cache
                    const response = await fetch(config.url, { cache: "no-store" });
                    
                    if (!response.ok) throw new Error(`Erro HTTP ${response.status}`);
                    
                    const text = await response.text();
                    reportDataCache[config.id] = parseRobustCSV(text);
                    
                    console.log(`‚úÖ Sucesso: ${config.label} (${reportDataCache[config.id].length} linhas)`);
                } catch (err) {
                    console.error(`‚ùå Erro em ${config.label}:`, err);
                    reportDataCache[config.id] = [];
                }
            });

            await Promise.all(promises);
            
            // SALVAR no localStorage para persist√™ncia
            try {
                localStorage.setItem('googleSheetsCache', JSON.stringify(reportDataCache));
                localStorage.setItem('googleSheetsLastUpdate', new Date().toISOString());
                console.log('üíæ Dados salvos no localStorage');
            } catch (e) {
                console.warn('‚ö†Ô∏è Erro ao salvar cache:', e);
            }
            
            // Remover mensagem de carregamento
            const loadingMsg = document.getElementById('loading-reports-message');
            if (loadingMsg) {
                loadingMsg.remove();
            }
            
            // Adicionar mensagem de sucesso
            if (chatMessages) {
                const successMsg = document.createElement('div');
                successMsg.className = 'message bot-message';
                successMsg.innerHTML = `
                    <div class="message-content">
                        <div class="message-avatar">ü§ñ</div>
                        <div class="message-text">
                            ‚úÖ ${Object.keys(reportDataCache).length} relat√≥rios carregados! Digite <strong>ajuda</strong> para comandos.
                        </div>
                    </div>
                `;
                chatMessages.appendChild(successMsg);
                chatMessages.scrollTop = chatMessages.scrollHeight;
            }
            
            console.log("‚úÖ Todos os dados foram carregados e est√£o em 'reportDataCache'.");
            console.log("üìä Total de relat√≥rios:", Object.keys(reportDataCache).length);
            return reportDataCache;
        }

        // =================================================================================
        // VARI√ÅVEIS GLOBAIS
        // =================================================================================
        let currentUser = null;
        let authToken = null;
        
