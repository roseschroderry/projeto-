        // Exportar relat√≥rio do cache
        function exportReportFromCache(reportId) {
            const data = reportDataCache[reportId];
            const config = REPORTS_CONFIG.find(r => r.id === reportId);
            
            if (!data || data.length === 0) {
                alert('‚ùå N√£o h√° dados para exportar');
                return;
            }
            
            const headers = Object.keys(data[0]);
            const csv = [headers.join(',')];
            
            data.forEach(row => {
                const values = headers.map(h => {
                    const val = row[h] || '';
                    return `"${String(val).replace(/"/g, '""')}"`;
                });
                csv.push(values.join(','));
            });
            
            const blob = new Blob([csv.join('\\n')], { type: 'text/csv;charset=utf-8;' });
            const url = URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = `${reportId}_${new Date().toISOString().split('T')[0]}.csv`;
            a.click();
            URL.revokeObjectURL(url);
            
            alert(`‚úÖ Relat√≥rio exportado!\\n\\nüìÑ ${config?.label || reportId}\\nüìä ${data.length} registros`);
        }
        
