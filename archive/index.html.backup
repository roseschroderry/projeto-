        function filtrarMSL(data, codvd, vendedor, tipoMSL) {
            console.log(`üîç Filtrando MSL ${tipoMSL} para CODVD=${codvd}, VENDEDOR=${vendedor || 'TODOS'}`);
            
            const colunaMSL = identificarColunaMSL(data);
            
            if (!colunaMSL) {
