            const isAdmin = currentUser.role === 'admin';
            
            const fileData = {
                name: file.name,
                size: file.size,
                type: file.type || getFileType(file.name),
                uploadDate: new Date().toISOString(),
                analysis: analysis,
                user: currentUser.email,
                userName: currentUser.name,
                sharedWithAll: isAdmin, // Admin compartilha com todos
                uploadedBy: isAdmin ? 'admin' : 'user'
            };
            
            store.add(fileData);
            
            transaction.oncomplete = () => {
                updateDatabaseStats();
                updateAnalytics();
                logAction('file_upload', `Arquivo ${file.name} enviado${isAdmin ? ' e compartilhado com todos' : ''}`);
