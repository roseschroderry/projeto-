        // Gerar gr√°fico visual a partir dos dados
        function generateChart(data, reportType) {
            if (!data || data.length === 0) return '';
            
            const headers = Object.keys(data[0]);
            if (headers.length < 2) return '';
            
            // Pegar primeira coluna como label e segunda como valor
            const labelKey = headers[0];
            const valueKey = headers[1];
            
            // Processar dados para o gr√°fico (limitar a 10 itens)
            const chartData = data.slice(0, 10).map(row => ({
                label: String(row[labelKey] || 'Sem nome').substring(0, 30),
                value: parseFloat(row[valueKey]) || 0
            }));
            
            // Encontrar valor m√°ximo para escala
            const maxValue = Math.max(...chartData.map(d => d.value), 1);
            
            let chart = `<div style="background: white; padding: 16px; border-radius: 8px; margin: 16px 0;">\n`;
            chart += `<h4 style="margin: 0 0 12px 0; color: #374151; font-size: 15px;">üìà Gr√°fico Visual</h4>\n`;
            
            chartData.forEach((item, index) => {
                const percentage = (item.value / maxValue) * 100;
                const color = index % 2 === 0 ? '#DC2626' : '#B91C1C';
                
                chart += `<div style="margin-bottom: 12px;">\n`;
                chart += `  <div style="display: flex; justify-content: space-between; margin-bottom: 4px;">\n`;
                chart += `    <span style="font-size: 12px; color: #6B7280; font-weight: 500;">${item.label}</span>\n`;
                chart += `    <span style="font-size: 12px; color: #111827; font-weight: 600;">${item.value.toLocaleString('pt-BR')}</span>\n`;
                chart += `  </div>\n`;
                chart += `  <div style="width: 100%; height: 24px; background: #F3F4F6; border-radius: 6px; overflow: hidden;">\n`;
                chart += `    <div style="width: ${percentage}%; height: 100%; background: linear-gradient(135deg, ${color}, ${color}dd); display: flex; align-items: center; justify-content: flex-end; padding-right: 8px; transition: width 0.3s ease;">\n`;
                chart += `      <span style="color: white; font-size: 11px; font-weight: 600;">${percentage.toFixed(0)}%</span>\n`;
                chart += `    </div>\n`;
                chart += `  </div>\n`;
                chart += `</div>\n`;
            });
            
            if (data.length > 10) {
                chart += `<p style="margin: 12px 0 0 0; font-size: 12px; color: #6B7280; text-align: center;"><em>Mostrando top 10 de ${data.length} registros</em></p>\n`;
            }
            
            chart += `</div>\n\n`;
            
            return chart;
        }

