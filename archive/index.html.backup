        // Fun√ß√£o de registro de novo usu√°rio
        function handleRegister(event) {
            event.preventDefault();

            const name = document.getElementById('registerName').value.trim();
            const email = document.getElementById('registerEmail').value.trim().toLowerCase();
            const password = document.getElementById('registerPassword').value;
            const passwordConfirm = document.getElementById('registerPasswordConfirm').value;

            // Valida√ß√µes
            if (!name || !email || !password) {
                showNotification('Por favor, preencha todos os campos', 'error');
                return false;
            }

            if (password !== passwordConfirm) {
                showNotification('As senhas n√£o coincidem', 'error');
                return false;
            }

            if (password.length < 6) {
                showNotification('A senha deve ter pelo menos 6 caracteres', 'error');
                return false;
            }

            // Verificar se o email j√° existe
            const users = JSON.parse(localStorage.getItem('registeredUsers') || '[]');
            const emailExists = users.some(u => u.email === email);

            if (emailExists) {
                showNotification('Este email j√° est√° registrado', 'error');
                return false;
            }

            // Criar novo usu√°rio
            const newUser = {
                name: name,
                email: email,
                password: password,
                role: 'user', // Usu√°rios novos s√£o sempre 'user', n√£o 'admin'
                createdAt: new Date().toISOString()
            };

            // Salvar no localStorage
            users.push(newUser);
            localStorage.setItem('registeredUsers', JSON.stringify(users));

            // Mostrar sucesso e redirecionar para login
            showNotification('‚úÖ Conta criada com sucesso! Fa√ßa login agora.', 'success');
            
            // Limpar formul√°rio
            document.getElementById('registerName').value = '';
            document.getElementById('registerEmail').value = '';
            document.getElementById('registerPassword').value = '';
            document.getElementById('registerPasswordConfirm').value = '';

            // Voltar para tela de login ap√≥s 1.5 segundos
            setTimeout(() => {
                showLoginForm();
                // Preencher email no login
                document.getElementById('loginEmail').value = email;
                document.getElementById('loginPassword').value = '';
            }, 1500);

            return false;
        }
