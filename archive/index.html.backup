        // =================================================================================
        // DETEC√á√ÉO AUTOM√ÅTICA DE COLUNAS (adaptado do Python)
        // =================================================================================
        function normalizarNomeColuna(nome) {
            return String(nome || '').toUpperCase().trim()
                .replace(/[√Ä√Å√Ç√É√Ñ√Ö]/g, 'A')
                .replace(/[√à√â√ä√ã]/g, 'E')
                .replace(/[√å√ç√é√è]/g, 'I')
                .replace(/[√í√ì√î√ï√ñ]/g, 'O')
                .replace(/[√ô√ö√õ√ú]/g, 'U')
                .replace(/[√á]/g, 'C')
                .replace(/\s+/g, ' ');
        }

        function encontrarColuna(row, variacoes) {
            for (const variacao of variacoes) {
                for (const [key, value] of Object.entries(row)) {
                    const normalizado = normalizarNomeColuna(key);
                    if (normalizado.includes(variacao) || variacao.includes(normalizado)) {
                        return value;
                    }
                }
            }
            return '';
        }

