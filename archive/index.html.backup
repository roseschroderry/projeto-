        // Carregar estat√≠sticas
        function loadUsageStats() {
            const container = document.getElementById('usageStats');
            if (!container) return;

            const sessions = JSON.parse(localStorage.getItem('userSessions') || '[]');

            if (sessions.length === 0) {
                container.innerHTML = `
                    <div style="text-align: center; padding: 60px 20px; color: var(--text-secondary);">
                        <p style="font-size: 64px; margin-bottom: 20px;">‚è±Ô∏è</p>
                        <p style="font-size: 18px; font-weight: 600; margin-bottom: 8px;">Nenhuma sess√£o registrada</p>
                        <p style="font-size: 14px;">Os dados aparecer√£o aqui conforme voc√™ usa o sistema</p>
                    </div>
                `;
                return;
            }

            const userStats = {};
            sessions.forEach(session => {
                if (!userStats[session.user]) {
                    userStats[session.user] = { totalTime: 0, sessions: 0, lastAccess: session.startTime };
                }
                const time = session.endTime ? (session.endTime - session.startTime) : (Date.now() - session.startTime);
                userStats[session.user].totalTime += time;
                userStats[session.user].sessions++;
                if (session.startTime > userStats[session.user].lastAccess) {
                    userStats[session.user].lastAccess = session.startTime;
                }
            });

            let html = '<table class="stats-table"><thead><tr><th>üë§ Usu√°rio</th><th>‚è±Ô∏è Tempo Total</th><th>üìä Sess√µes</th><th>üìÖ √öltimo Acesso</th></tr></thead><tbody>';

            Object.entries(userStats).forEach(([user, stats]) => {
                const hours = Math.floor(stats.totalTime / 3600000);
                const minutes = Math.floor((stats.totalTime % 3600000) / 60000);
                const seconds = Math.floor((stats.totalTime % 60000) / 1000);
                const timeDisplay = hours > 0 ? `${hours}h ${minutes}min` : minutes > 0 ? `${minutes}min ${seconds}s` : `${seconds}s`;
                const lastAccess = new Date(stats.lastAccess).toLocaleString('pt-BR');

                html += `
                    <tr>
                        <td><strong>${user}</strong></td>
                        <td style="text-align: center; color: var(--red-primary); font-weight: 700;">${timeDisplay}</td>
                        <td style="text-align: center;">${stats.sessions}</td>
                        <td style="font-size: 13px; color: var(--text-secondary);">${lastAccess}</td>
                    </tr>
                `;
            });

            html += '</tbody></table>';
            container.innerHTML = html;
        }

        // Atualizar stats do dashboard
        function updateDashboardStats() {
            const users = JSON.parse(localStorage.getItem('registeredUsers') || '[]');
            document.getElementById('totalUsers').textContent = users.length;

            const sessions = JSON.parse(localStorage.getItem('userSessions') || '[]');
            if (sessions.length > 0) {
                const totalTime = sessions.reduce((sum, s) => {
                    const time = s.endTime ? (s.endTime - s.startTime) : (Date.now() - s.startTime);
                    return sum + time;
                }, 0);
                const avgMinutes = Math.floor(totalTime / sessions.length / 60000);
                document.getElementById('avgTime').textContent = avgMinutes + 'min';
            }
        }

